@article{otpinventor,
	author = { Steven M.   Bellovin },
	title = {Frank Miller: Inventor of the One-Time Pad},
	journal = {Cryptologia},
	volume = {35},
	number = {3},
	pages = {203-222},
	year = {2011},
	doi = {10.1080/01611194.2011.583711},
	url = {http://dx.doi.org/10.1080/01611194.2011.583711},
	eprint = {http://dx.doi.org/10.1080/01611194.2011.583711},
    abstract = { Abstract The invention of the one-time pad is generally credited to Gilbert S. Vernam and Joseph O. Mauborgne. We show that it was invented about 35 years earlier by a Sacramento banker named Frank Miller. We identify which Frank Miller it was, and speculate on what might have led him to his idea. We also discuss whether or not Mauborgne might have known of Miller's work, especially via his colleague Parker Hitt. },
}


